{
        "AWSEBDockerrunVersion": 1,
        "volumes": [
                {
                        "name": "node-test-api",
                        "host": {
                          "sourcePath": "/var/app/current/declic-idea-api"
                        }
                }
        ],
        "containerDefinitions": [
                {
                        "name": "csaas-node-test-api",
                        "image": "622142464602.dkr.ecr.eu-west-3.amazonaws.com/csaas-node-test-api:latest",
                        "essential": true,
                        "memory": 128,
                        "mountPoints": [
                          {
                                  "sourceVolume": "csaas-node-test-api",
                                  "containerPath": "/usr/local/app",
                                  "readOnly": true
                          }
                        ]
                }
        ]

}
