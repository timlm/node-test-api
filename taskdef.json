{
  "executionRoleArn": "arn:aws:iam::622142464602:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "node-test-container",
      "image": "<IMAGE1_NAME>",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 3000,
          "protocol": "tcp",
          "containerPort": 3000
        }
      ]
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512",
  "family": "node-test-bluegreen"
}